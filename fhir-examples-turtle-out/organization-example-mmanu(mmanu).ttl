@prefix fhir: <http://hl7.org/fhir/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

<http://hl7.org/fhir/Organization/mmanu> a fhir:Organization ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "mmanu"] ; # 
  fhir:meta [
     fhir:security ( [
       fhir:system [ fhir:v "http://terminology.hl7.org/CodeSystem/v3-ActReason"^^xsd:anyURI ] ;
       fhir:code [ fhir:v "HTEST" ] ;
       fhir:display [ fhir:v "test health data" ]
     ] )
  ] ; # 
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\">\n      <p>Acme Corporation, Proud member of the pharma industry</p>\n    </div>"^^rdf:XMLLiteral
  ] ; # 
  fhir:active [ fhir:v "true"^^xsd:boolean] ; # 
  fhir:name [ fhir:v "Acme Corporation"] ; # 
  fhir:contact ( [
     fhir:address [
       fhir:country [ fhir:v "Swizterland" ]
     ]
  ] ) . # 

