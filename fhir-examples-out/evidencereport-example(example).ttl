@prefix fhir: <http://hl7.org/fhir/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

<http://hl7.org/fhir/EvidenceReport/example> a fhir:EvidenceReport ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "example"] ; # 
  fhir:meta [
     fhir:security ( [
       fhir:system [
         fhir:v "http://terminology.hl7.org/CodeSystem/v3-ActReason"^^xsd:anyURI ;
         fhir:link <http://terminology.hl7.org/CodeSystem/v3-ActReason>
       ] ;
       fhir:code [ fhir:v "HTEST" ] ;
       fhir:display [ fhir:v "test health data" ]
     ] )
  ] ; # 
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\">[Put rendering here]</div>"^^rdf:XMLLiteral
  ] ; # 
  fhir:status [ fhir:v "draft"] ; # 
  fhir:subject [
     fhir:note ( [
       fhir:text [ fhir:v "This is just an example." ]
     ] )
  ] . # 

